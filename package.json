{
  "name": "calendar-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fontsource/roboto": "^4.5.8",
    "@mui/material": "^5.10.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.1",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.6",
    "env-cmd": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.4",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "export NODE_OPTIONS=--max_old_space_size=2048 && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:dev": "env-cmd -f .env.development npm run build",
    "build:staging": "env-cmd -f .env.staging npm run build",
    "build:uat": "env-cmd -f .env.uat npm run build",
    "build:prod": "env-cmd -f .env.production npm run build",
    "build:altstaging": "env-cmd -f .env.altstaging npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.1",
    "@types/uuid": "^8.3.4",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1",
    "web-vitals": "^2.1.4"
  }
}
